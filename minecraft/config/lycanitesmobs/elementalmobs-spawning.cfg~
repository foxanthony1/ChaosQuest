# Configuration file

##########################################################################################################
# area limits
#--------------------------------------------------------------------------------------------------------#
# Sets how many of each mob is allowed to naturally spawn near each other.
##########################################################################################################

"area limits" {
    I:"Argus Area Limit"=3
    I:"Cinder Area Limit"=3
    I:"Djinn Area Limit"=3
    I:"Geonach Area Limit"=5
    I:"Grue Area Limit"=5
    I:"Jengu Area Limit"=3
    I:"Reiver Area Limit"=3
    I:"Spectre Area Limit"=4
    I:"Spriggan Area Limit"=3
    I:"Tremor Area Limit"=3
    I:"Volcan Area Limit"=2
    I:"Wraith Area Limit"=10
    I:"Zephyr Area Limit"=3
}


##########################################################################################################
# block costs
#--------------------------------------------------------------------------------------------------------#
# Only used by certain spawners. Sets how many blocks are required for spawning, such as how many blocks of Fire a Cinder requires.
##########################################################################################################

"block costs" {
    I:"Argus Block Cost"=1
    I:"Cinder Block Cost"=8
    I:"Djinn Block Cost"=1
    I:"Geonach Block Cost"=1
    I:"Grue Block Cost"=1
    I:"Jengu Block Cost"=1
    I:"Reiver Block Cost"=8
    I:"Spectre Block Cost"=1
    I:"Spriggan Block Cost"=1
    I:"Tremor Block Cost"=1
    I:"Volcan Block Cost"=1
    I:"Wraith Block Cost"=1
    I:"Zephyr Block Cost"=1
}


##########################################################################################################
# despawning
#--------------------------------------------------------------------------------------------------------#
# Forces a mob to despawn naturally (unless tamed). Some farmable mobs such as Pinkies ignore their Natural Despawning setting once they've been fed or moved out of their home dimension.
##########################################################################################################

despawning {
    B:"Argus Forced Despawning"=false
    B:"Argus Natural Despawning"=true
    B:"Cinder Forced Despawning"=false
    B:"Cinder Natural Despawning"=true
    B:"Djinn Forced Despawning"=false
    B:"Djinn Natural Despawning"=true
    B:"Geonach Forced Despawning"=false
    B:"Geonach Natural Despawning"=true
    B:"Grue Forced Despawning"=false
    B:"Grue Natural Despawning"=true
    B:"Jengu Forced Despawning"=false
    B:"Jengu Natural Despawning"=true
    B:"Reiver Forced Despawning"=false
    B:"Reiver Natural Despawning"=true
    B:"Spectre Forced Despawning"=false
    B:"Spectre Natural Despawning"=true
    B:"Spriggan Forced Despawning"=false
    B:"Spriggan Natural Despawning"=true
    B:"Tremor Forced Despawning"=false
    B:"Tremor Natural Despawning"=true
    B:"Volcan Forced Despawning"=false
    B:"Volcan Natural Despawning"=true
    B:"Wraith Forced Despawning"=false
    B:"Wraith Natural Despawning"=true
    B:"Zephyr Forced Despawning"=false
    B:"Zephyr Natural Despawning"=true
}


##########################################################################################################
# disable subspecies spawns
#--------------------------------------------------------------------------------------------------------#
# Set to true to prevent mobs from spawning as a subspecies (this doesn't remove subspecies, it just prevents them from naturally spawning).
##########################################################################################################

"disable subspecies spawns" {
    B:"Argus Subspecies Spawning Disabled"=false
    B:"Cinder Subspecies Spawning Disabled"=false
    B:"Djinn Subspecies Spawning Disabled"=false
    B:"Geonach Subspecies Spawning Disabled"=false
    B:"Grue Subspecies Spawning Disabled"=false
    B:"Jengu Subspecies Spawning Disabled"=false
    B:"Reiver Subspecies Spawning Disabled"=false
    B:"Spectre Subspecies Spawning Disabled"=false
    B:"Spriggan Subspecies Spawning Disabled"=false
    B:"Tremor Subspecies Spawning Disabled"=false
    B:"Volcan Subspecies Spawning Disabled"=false
    B:"Wraith Subspecies Spawning Disabled"=false
    B:"Zephyr Subspecies Spawning Disabled"=false
}


##########################################################################################################
# dungeon weights
#--------------------------------------------------------------------------------------------------------#
# The higher the weight, the more likely this mob will appear in random dungeon spawners. Vanilla Zombie have a dungeon weight of 200.
##########################################################################################################

"dungeon weights" {
    D:"Argus Dungeon Weight"=100.0
    D:"Cinder Dungeon Weight"=200.0
    D:"Djinn Dungeon Weight"=150.0
    D:"Geonach Dungeon Weight"=100.0
    D:"Grue Dungeon Weight"=200.0
    D:"Jengu Dungeon Weight"=200.0
    D:"Reiver Dungeon Weight"=200.0
    D:"Spectre Dungeon Weight"=50.0
    D:"Spriggan Dungeon Weight"=100.0
    D:"Tremor Dungeon Weight"=25.0
    D:"Volcan Dungeon Weight"=50.0
    D:"Wraith Dungeon Weight"=120.0
    D:"Zephyr Dungeon Weight"=200.0
}


##########################################################################################################
# enabled spawns
#--------------------------------------------------------------------------------------------------------#
# Set to false to prevent mobs from spawning naturally at all.
##########################################################################################################

"enabled spawns" {
    B:"Argus Spawning Enabled"=true
    B:"Cinder Spawning Enabled"=true
    B:"Djinn Spawning Enabled"=true
    B:"Geonach Spawning Enabled"=true
    B:"Grue Spawning Enabled"=true
    B:"Jengu Spawning Enabled"=true
    B:"Reiver Spawning Enabled"=true
    B:"Spectre Spawning Enabled"=true
    B:"Spriggan Spawning Enabled"=true
    B:"Tremor Spawning Enabled"=true
    B:"Volcan Spawning Enabled"=true
    B:"Wraith Spawning Enabled"=true
    B:"Zephyr Spawning Enabled"=true
}


##########################################################################################################
# group settings
#--------------------------------------------------------------------------------------------------------#
# Here you can set the spawning settings for all mobs in this group that use the GROUP tag.
##########################################################################################################

"group settings" {
    # Sets which biomes this mob spawns in using Biome Tags. Multiple entries should be comma separated and can be subtractive if provided with a - in front.
    S:"Elemental Mobs Spawn Biomes"=ALL
    S:"Elemental Mobs Spawn Dimensions"=

    # If true, the 'Spawn Dimensions' list will act as a whitelist instead of a blacklist.
    B:"Elemental Mobs Spawn Dimensions Whitelist Mode"=false

    # Here you can set the Dungeon Theme of this mob group. These are used by Doomlike Dungeons and might be used by other things later. Multiple entries should be comma seperated.
    S:"Elemental Mobs Themes"=
}


##########################################################################################################
# group sizes
#--------------------------------------------------------------------------------------------------------#
# Sets the minimum and maximum random size of a group spawned. Note with the vanilla spawn, large groups can reduce the spawning odds in biomes with lots trees, etc.
##########################################################################################################

"group sizes" {
    I:"Argus Group Max"=3
    I:"Argus Group Min"=1
    I:"Cinder Group Max"=3
    I:"Cinder Group Min"=1
    I:"Djinn Group Max"=3
    I:"Djinn Group Min"=1
    I:"Geonach Group Max"=2
    I:"Geonach Group Min"=1
    I:"Grue Group Max"=2
    I:"Grue Group Min"=1
    I:"Jengu Group Max"=3
    I:"Jengu Group Min"=1
    I:"Reiver Group Max"=3
    I:"Reiver Group Min"=1
    I:"Spectre Group Max"=2
    I:"Spectre Group Min"=1
    I:"Spriggan Group Max"=3
    I:"Spriggan Group Min"=1
    I:"Tremor Group Max"=2
    I:"Tremor Group Min"=1
    I:"Volcan Group Max"=2
    I:"Volcan Group Min"=1
    I:"Wraith Group Max"=4
    I:"Wraith Group Min"=1
    I:"Zephyr Group Max"=3
    I:"Zephyr Group Min"=1
}


##########################################################################################################
# spawn biomes
#--------------------------------------------------------------------------------------------------------#
# Sets which biomes this mob spawns in using Biome Tags. Multiple entries should be comma separated and can be subtractive if provided with a - in front. You can also have a mob skip the biome check completely if ALL is not working correctly for modded biomes.
##########################################################################################################

"spawn biomes" {
    S:"Argus Spawn Biomes"=GROUP
    B:"Argus Spawn Ignores Biome Check"=false
    S:"Cinder Spawn Biomes"=GROUP
    B:"Cinder Spawn Ignores Biome Check"=false
    S:"Djinn Spawn Biomes"=GROUP
    B:"Djinn Spawn Ignores Biome Check"=false
    S:"Geonach Spawn Biomes"=GROUP
    B:"Geonach Spawn Ignores Biome Check"=false
    S:"Grue Spawn Biomes"=ALL
    B:"Grue Spawn Ignores Biome Check"=false
    S:"Jengu Spawn Biomes"=GROUP
    B:"Jengu Spawn Ignores Biome Check"=false
    S:"Reiver Spawn Biomes"=COLD, SNOWY, CONIFEROUS, -END
    B:"Reiver Spawn Ignores Biome Check"=false
    S:"Spectre Spawn Biomes"=ALL
    B:"Spectre Spawn Ignores Biome Check"=false
    S:"Spriggan Spawn Biomes"=GROUP
    B:"Spriggan Spawn Ignores Biome Check"=false
    S:"Tremor Spawn Biomes"=GROUP
    B:"Tremor Spawn Ignores Biome Check"=false
    S:"Volcan Spawn Biomes"=GROUP
    B:"Volcan Spawn Ignores Biome Check"=false
    S:"Wraith Spawn Biomes"=GROUP
    B:"Wraith Spawn Ignores Biome Check"=false
    S:"Zephyr Spawn Biomes"=GROUP
    B:"Zephyr Spawn Ignores Biome Check"=false
}


##########################################################################################################
# spawn chances
#--------------------------------------------------------------------------------------------------------#
# Adds a forced spawn fail chance. A chance of 1.0 (100%) means that the mob will always spawn. A chance of 0.5 means it will have a 50% chance of spawning. Use this as a last resort as a failed spawn still takes up a spawning oppotunity.
##########################################################################################################

"spawn chances" {
    D:"Argus Spawn Chance"=1.0
    D:"Cinder Spawn Chance"=1.0
    D:"Djinn Spawn Chance"=1.0
    D:"Geonach Spawn Chance"=1.0
    D:"Grue Spawn Chance"=1.0
    D:"Jengu Spawn Chance"=1.0
    D:"Reiver Spawn Chance"=1.0
    D:"Spectre Spawn Chance"=1.0
    D:"Spriggan Spawn Chance"=1.0
    D:"Tremor Spawn Chance"=1.0
    D:"Volcan Spawn Chance"=1.0
    D:"Wraith Spawn Chance"=1.0
    D:"Zephyr Spawn Chance"=1.0
}


##########################################################################################################
# spawn conditions
#--------------------------------------------------------------------------------------------------------#
# Various conditions that are checked at a spawn location when spawning mobs, such as light level.
##########################################################################################################

"spawn conditions" {
    B:"Argus Spawns In Dark"=true
    B:"Argus Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Argus World Day Minimum"=0
    B:"Cinder Spawns In Dark"=true
    B:"Cinder Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Cinder World Day Minimum"=0
    B:"Djinn Spawns In Dark"=true
    B:"Djinn Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Djinn World Day Minimum"=0
    B:"Geonach Spawns In Dark"=true
    B:"Geonach Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Geonach World Day Minimum"=0
    B:"Grue Spawns In Dark"=true
    B:"Grue Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Grue World Day Minimum"=0
    B:"Jengu Spawns In Dark"=true
    B:"Jengu Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Jengu World Day Minimum"=0
    B:"Reiver Spawns In Dark"=true
    B:"Reiver Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Reiver World Day Minimum"=0
    B:"Spectre Spawns In Dark"=true
    B:"Spectre Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Spectre World Day Minimum"=0
    B:"Spriggan Spawns In Dark"=true
    B:"Spriggan Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Spriggan World Day Minimum"=0
    B:"Tremor Spawns In Dark"=true
    B:"Tremor Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Tremor World Day Minimum"=0
    B:"Volcan Spawns In Dark"=true
    B:"Volcan Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Volcan World Day Minimum"=0
    B:"Wraith Spawns In Dark"=true
    B:"Wraith Spawns In Light"=true

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Wraith World Day Minimum"=0
    B:"Zephyr Spawns In Dark"=true
    B:"Zephyr Spawns In Light"=false

    # How many days old the world must at least be for this mob to be able to naturally spawn.
    I:"Zephyr World Day Minimum"=0
}


##########################################################################################################
# spawn dimensions
#--------------------------------------------------------------------------------------------------------#
# Sets which dimensions (by ID) that mobs WILL NOT spawn in. However if 'Spawn Dimensions Whitelist Mode' is set to true, it will instead set which dimensions they WILL ONLY spawn in. You may enter dimension IDs and/or GROUP to use the group settings. Multiple entries should be comma separated. Note that some Spawn Types ignore this such as the PORTAL type.
##########################################################################################################

"spawn dimensions" {
    S:"Argus Spawn Dimensions"=GROUP
    B:"Argus Spawn Dimensions Whitelist Mode"=false
    S:"Cinder Spawn Dimensions"=GROUP
    B:"Cinder Spawn Dimensions Whitelist Mode"=false
    S:"Djinn Spawn Dimensions"=-1, 1
    B:"Djinn Spawn Dimensions Whitelist Mode"=false
    S:"Geonach Spawn Dimensions"=GROUP
    B:"Geonach Spawn Dimensions Whitelist Mode"=false
    S:"Grue Spawn Dimensions"=-1
    B:"Grue Spawn Dimensions Whitelist Mode"=false
    S:"Jengu Spawn Dimensions"=GROUP
    B:"Jengu Spawn Dimensions Whitelist Mode"=false
    S:"Reiver Spawn Dimensions"=GROUP
    B:"Reiver Spawn Dimensions Whitelist Mode"=false
    S:"Spectre Spawn Dimensions"=1
    B:"Spectre Spawn Dimensions Whitelist Mode"=true
    S:"Spriggan Spawn Dimensions"=GROUP
    B:"Spriggan Spawn Dimensions Whitelist Mode"=false
    S:"Tremor Spawn Dimensions"=GROUP
    B:"Tremor Spawn Dimensions Whitelist Mode"=false
    S:"Volcan Spawn Dimensions"=GROUP
    B:"Volcan Spawn Dimensions Whitelist Mode"=false
    S:"Wraith Spawn Dimensions"=GROUP
    B:"Wraith Spawn Dimensions Whitelist Mode"=false
    S:"Zephyr Spawn Dimensions"=GROUP
    B:"Zephyr Spawn Dimensions Whitelist Mode"=false
}


##########################################################################################################
# spawn types
#--------------------------------------------------------------------------------------------------------#
# Specifies how this mob spawns, multiple entries should be comma separated. You can use the name or title property of any json spawner or these vanilla spawners: MONSTER, CREATURE.
##########################################################################################################

"spawn types" {
    S:"Argus Spawn Types"=
    S:"Cinder Spawn Types"=
    S:"Djinn Spawn Types"=
    S:"Geonach Spawn Types"=
    S:"Grue Spawn Types"=UNDERGROUND
    S:"Jengu Spawn Types"=WATER
    S:"Reiver Spawn Types"=SKY
    S:"Spectre Spawn Types"=SKY
    S:"Spriggan Spawn Types"=
    S:"Tremor Spawn Types"=
    S:"Volcan Spawn Types"=
    S:"Wraith Spawn Types"=PORTAL, NETHERSKY
    S:"Zephyr Spawn Types"=
}


##########################################################################################################
# spawn weights
#--------------------------------------------------------------------------------------------------------#
# The higher the weight, the more likely the mob will spawn randomly instead of others. Vanilla Zombies have a weight of 8.
##########################################################################################################

"spawn weights" {
    D:"Argus Spawn Weight"=4.0
    D:"Cinder Spawn Weight"=8.0
    D:"Djinn Spawn Weight"=6.0
    D:"Geonach Spawn Weight"=4.0
    D:"Grue Spawn Weight"=8.0
    D:"Jengu Spawn Weight"=8.0
    D:"Reiver Spawn Weight"=8.0
    D:"Spectre Spawn Weight"=2.0
    D:"Spriggan Spawn Weight"=4.0
    D:"Tremor Spawn Weight"=1.0
    D:"Volcan Spawn Weight"=2.0
    D:"Wraith Spawn Weight"=6.0
    D:"Zephyr Spawn Weight"=8.0
}


